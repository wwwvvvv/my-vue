<%- include header.ejs %>
<%if (posts.length > 0) {%>
    <% posts.forEach(function (post, index) { %>
        <p><h2><%= post.title %></h2></p>
        <p class="info">
            作者：<a href="#"><%=post.name%></a> |
            日期：<%=post.time.time%>
        </p>
        <p><%-post.post%></p>
        <hr>
    <% }) %>
<%} else {%>
    <p>暂无博文</p>
<%}%>
<%- include footer.ejs %>